{
  "name": "vilare/theme",
  "description": "Vilare Theme",
  "version": "0.1.4",
  "license": "GPLv3",
  "autoload": {
    "psr-4": {
      "Vilare\\": "app/"
    }
  },
  "require": {
    "php": "^8.3",
    "illuminate/view": "^12.10",
    "illuminate/support": "^12.10"
  },
  "scripts": {
    "lint": "phpcs",
    "format": "phpcbf"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "^3.1.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
